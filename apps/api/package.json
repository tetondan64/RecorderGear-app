{
  "name": "recordergear-api",
  "version": "1.0.0",
  "description": "RecorderGear API server with S3-compatible cloud storage",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate:pg --config ./drizzle.config.ts",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "db:import-json": "tsx src/scripts/import-json-db.ts"
  },
  "keywords": [
    "api",
    "fastify",
    "s3",
    "presigned",
    "audio",
    "recording"
  ],
  "author": "RecorderGear",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/s3-request-presigner": "^3.478.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/env": "^4.2.0",
    "@types/jsonwebtoken": "^9.0.10",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.0",
    "fastify": "^4.24.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "drizzle-kit": "^0.20.4",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.1",
    "tsx": "^4.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
